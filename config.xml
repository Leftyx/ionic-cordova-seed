<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="co.uk.nsevenit.sampleapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Sample App</name>
  <description>
        Sample App
    </description>
  <author email="hello@nsevenit.co.uk" href="http://nsevenit.co.uk/">
      nSeven IT Ltd Team
    </author>
  <content src="index.html"/>
  <access origin="*"/>
